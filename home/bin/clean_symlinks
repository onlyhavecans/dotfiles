#!/usr/bin/env bash

##
# Clean up after removing things from homeshick castles
##

fd --hidden --follow --type l \
  --exclude .local \
  --exclude Code \
  --exclude Desktop \
  --exclude Documents \
  --exclude Library \
  --exclude Photos \
  --exclude Orbstack \
  --exclude ".nix-*" \
  . ~ \
  --exec trash-rm -v {}
